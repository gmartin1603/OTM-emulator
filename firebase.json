{
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 7000
    },
    "ui": {
      "enabled": true
    },
    "functions": {
      "port": 5001
    },
    "pubsub": {
      "port": 8085
    },
    "singleProjectMode": true
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "source": "functions",
      "codebase": "default"
    }
  ]
}
